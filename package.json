{
	"name": "af-klm-status",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prepare": "husky install",
		"postinstall": "paraglide-js compile --project ./project.inlang",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"lint-staged": {
		"*.{js,jsx,tsx,ts,svelte}": "eslint --fix",
		"*.{js,jsx,ts,tsx,md,html,css,svelte}": "prettier --write"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "^1.6.1",
		"@inlang/paraglide-js-adapter-vite": "^1.2.38",
		"@playwright/test": "^1.43.1",
		"@storybook/addon-essentials": "^8.0.8",
		"@storybook/addon-interactions": "^8.0.8",
		"@storybook/addon-links": "^8.0.8",
		"@storybook/addon-svelte-csf": "^4.1.2",
		"@storybook/blocks": "^8.0.8",
		"@storybook/svelte": "^8.0.8",
		"@storybook/sveltekit": "^8.0.8",
		"@storybook/test": "^8.0.8",
		"@svelte-plugins/tooltips": "^3.0.0",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/eslint": "^8.56.9",
		"@types/luxon": "^3.4.2",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-svelte": "^2.37.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"openapi-typescript": "^6.7.5",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^8.0.8",
		"svelte": "^4.2.14",
		"svelte-check": "^3.6.9",
		"svelte-eslint-parser": "^0.34.1",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.8",
		"vitest": "^1.5.0",
		"yarn-upgrade-all": "^0.7.2"
	},
	"type": "module",
	"packageManager": "yarn@4.0.2",
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.5.2",
		"@fortawesome/free-solid-svg-icons": "^6.5.2",
		"@svelte-plugins/datepicker": "^1.0.9",
		"date-picker-svelte": "^2.12.0",
		"dayjs": "^1.11.10",
		"luxon": "^3.4.4",
		"svelte-fa": "^4.0.2",
		"svelte-loading-spinners": "^0.3.6",
		"svelte-persisted-store": "^0.9.1"
	}
